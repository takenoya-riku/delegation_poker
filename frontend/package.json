{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "dev": "nuxt dev --port 8088",
    "generate": "nuxt generate",
    "lint": "eslint . --ext .js,.ts,.vue && vue-tsc --noEmit -p tsconfig.eslint.json",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix && vue-tsc --noEmit -p tsconfig.eslint.json",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/devtools": "latest",
    "@urql/vue": "^2.0.0",
    "graphql": "^16.8.1",
    "nuxt": "^3.13.0",
    "urql": "^5.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "daisyui": "^5.5.14",
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@storybook/addon-essentials": "^8.1.7",
    "@storybook/vue3-vite": "^8.1.7",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.1",
    "storybook": "^8.1.7",
    "tailwindcss": "^3.4.17",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.8",
    "vue-eslint-parser": "^9.4.3"
  }
}
